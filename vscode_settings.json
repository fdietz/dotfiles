{
  "window.zoomLevel": 0,
  "editor.fontSize": 15,
  "editor.fontFamily": "Fira Code",
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.labelFormat": "short",
  "workbench.colorTheme": "Tomorrow Night",
  "editor.minimap.enabled": false,
  "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/deps": true,
      "**/_build": true
  },
  "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/**": true,
      "**/deps/**": true,
      "**/_build/**": true
  },
  "workbench.editor.enablePreview": false,
  "editor.fontLigatures": false,
  "flow.useNPMPackagedFlow": true,
  "javascript.validate.enable": false,
  "terminal.integrated.fontSize": 14,
  "workbench.activityBar.visible": true,
  "emmet.includeLanguages": {
      "HTML (Eex)": "html",
      "vue": "html",
      "vue-html": "html",
      "javascriptreact": "html",
      "erb": "html"
  },
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": false,
  "javascript.implicitProjectConfig.checkJs": true,
  "elixirLS.dialyzerEnabled": false,
  "editor.formatOnSave": false,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      {
          "language": "vue",
          "autoFix": true
      },
      {
          "language": "html",
          "autoFix": true
      },
      "vue"
  ],
  "javascript.updateImportsOnFileMove.enabled": "never",
  "editor.folding": false,
  "window.title": "${activeEditorMedium}${separator}${rootName}${separator}${rootPath}",
  "breadcrumbs.enabled": false,
  "elixirLS.fetchDeps": false,
  "auto-close-tag.activationOnLanguage": [
      "xml",
      "php",
      "blade",
      "ejs",
      "jinja",
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "plaintext",
      "markdown",
      "vue",
      "liquid",
      "ruby",
      "erb"
  ],
  "todo-tree.highlights.enabled": true,
  "vsonline.planFilter": "/subscriptions/4e022397-fb03-48ac-b355-4ec3406a5723/resourceGroups/vso-rg-aa4957c/providers/Microsoft.VSOnline/plans/vso-plan-westeurope"
}